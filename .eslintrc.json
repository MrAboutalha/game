{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb","prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6
    },
    "plugins": [
        "react","prettier"
    ],
    "rules": {
        "prettier/prettier":"error",
        "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }]
    }
}
